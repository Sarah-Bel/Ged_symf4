{% extends 'base.html.twig' %}

{% block title %} {% endblock %}
{% block body %}
<div class="container mt-4">
 <div class="col-xs-10">
   
 {{ form_start(form,{'attr':{'novalidate':'novalidate'}}) }}
 {{ form_row(form) }}

<table>
<form >
    <div >
    
        <div class="col">
        
         {{ form_widget(form.numdocument) }}

        </div>

        <div class="col">
            {{ form_widget(form.information) }}
        </div>
         <div class="col">
            {{ form_widget(form.commentaire) }}
        </div>
         <div class="col">
            {{ form_widget(form.nomdocument) }}
        </div>
        <div class="col">
            {{ form_widget(form.imageFile) }}
        </div>
        
    </div>

    
    <div>
      <button type="submit" class="btn btn-primary mb-2">Enregistre
    </div>

</div>

</form>
 </div>  

 </div>  
{% endblock %}

