{% extends 'base.html.twig' %}

{% block title %}Documents {% endblock %}
{% block body %}
<div class="container mt-4">
 <div class="col-xs-10">
   
 {{ form_start(form,{'attr':{'novalidate':'novalidate'}}) }}
 {{ form_widget(form) }}

<form>
    
    <div class="row">
    
        <div class="col">
            {{ form_row(form.numdocument) }}

        </div>

        <div class="col">
            {{ form_row(form.information) }}
        </div>
         <div class="col">
            {{ form_row(form.commentaire) }}
        </div>
         <div class="col">
            {{ form_row(form.nomdocument) }}
        </div>
        <div class="col">
            {{ form_row(form.imageFile) }}
        </div>
        
    </div>

    
    <div>
      <button type="submit" class="btn btn-primary mb-2">Modifier
    </div>

</div>
</form>
   </div>
   </div>
{% endblock %}

